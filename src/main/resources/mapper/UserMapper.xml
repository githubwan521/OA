<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="motian.dao.mapper.UserMapper">
    <resultMap id="UserType" type="UserData">
        <result property="userId" column="user_id"/>
        <result property="nickname" column="nickname"/>
        <result property="identity" column="identity"/>
        <result property="department" column="department"/>
    </resultMap>

    <sql id="userColums">
        nickname, identity, department
    </sql>

    <insert id="insert">
        insert into OA_user
        (user_id,
        <include refid="userColums"/>
        ,db_create_time,db_update_time)
        values
        (#{data.userId},#{data.nickname},#{data.identity},
        #{data.department} ,now(),now());
    </insert>

</mapper>